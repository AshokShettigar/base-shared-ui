  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config ./configs/webpack.dev.config.js --progress --profile --colors",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.js --progress --profile --colors",
    "build:dt": "cross-env NODE_ENV=dt webpack --config ./webpack/webpack.platform-common-ui.dll.config.js --progress --colors --profile",
    "build:prod": "cross-env NODE_ENV='production' webpack --config ./webpack/webpack.platform-common-ui.dll.config.js --progress --colors --profile",
    "build:multi": "cross-env webpack --config ./webpack/webpack.platform-common-ui.dll.config.js --progress  --colors --profile",
    "clean": "rimraf coverage lib dist test-reports .eslintcache",
    "coverage": "cross-env ./node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint \"src/**\" \"example-app/app/**\"",
    "lint-no-cache": "eslint \"src/**\" \"example-app/app/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "start": "node ./example-app/server.js",
    "test": "jest",
    "test-update-snapshot": "npm run test -- -u",
    "test-watch": "npm run test -- --watch",
    "storybook": "start-storybook -p 9001 -c ./storybook/.storybook",
    "build:storybook": "build-storybook -c ./storybook/.storybook -o ./storybook/dist"
  },
  "jest": {
    "moduleNameMapper": {
      "^platform-common-ui(.*)$": "<rootDir>/src$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "notify": true,
    "setupTestFrameworkScriptFile": "./configs/jasmine-setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./configs/test-globals.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
